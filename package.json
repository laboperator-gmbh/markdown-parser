{
  "name": "@laboperator-gmbh/markdown-parser",
  "version": "1.0.0",
  "description": "Extended Markdown parser",
  "homepage": "https://github.com/laboperator-gmbh/markdown-parser#readme",
  "repository": {
    "type": "git",
    "url": "github:laboperator-gmbh/markdown-parser"
  },
  "license": "Apache-2.0",
  "author": "Laboperator GmbH",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc-alias",
    "ci:esm": "node ./bin/test",
    "ci:test": "NODE_OPTIONS=--experimental-vm-modules jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "lint": "eslint --fix 'src/**/*.{ts,js}'",
    "prepare": "husky",
    "sort": "sort-package-json ./package.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "dependencies": {
    "hast-util-sanitize": "^5.0.2",
    "lodash": "^4.17.23",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-to-hast": "^13.2.1",
    "micromark-core-commonmark": "^2.0.3",
    "micromark-factory-space": "^2.0.1",
    "micromark-util-character": "^2.1.1",
    "micromark-util-symbol": "^2.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@laboperator-gmbh/config": "~0.2.1",
    "@laboperator-gmbh/eslint-config-node": "^0.2.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.1.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "lint-staged": "^16.2.7",
    "micromark-util-types": "^2.0.2",
    "prettier": "^3.8.1",
    "sort-package-json": "^3.6.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.12.0"
}
